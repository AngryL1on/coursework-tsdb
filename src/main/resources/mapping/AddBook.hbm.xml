<hibernate-mapping>
    <class name="ru.rutmiit.lsm.entities.Books" table="Books">
        <property column="id" name="id" type="java.lang.Integer"/>
        <property column="title" name="title" type="java.lang.String"/>
        <property column="author" name="author" type="java.lang.String"/>
        <property column="publisher" name="publisher" type="java.lang.String"/>
        <property column="quantity" name="quantity" type="java.lang.Short"/>
        <property column="isAvail" name="isAvail" type="java.lang.Boolean"/>
    </class>
</hibernate-mapping>